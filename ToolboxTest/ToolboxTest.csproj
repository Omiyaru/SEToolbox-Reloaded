<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <AppDesignerFolder>Properties</AppDesignerFolder>
    <TargetFramework>net48</TargetFramework>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <UseWPF>true</UseWPF>
    <LangVersion>preview</LangVersion>
    <SignAssembly>false</SignAssembly>
    <Title>ToolboxTest</Title>
    <Product>ToolboxTest</Product>
    <Copyright>Copyright © MidSpace 2013-2018</Copyright>
    <Platforms>AnyCPU;x64</Platforms>
  </PropertyGroup>

  <ImportGroup Label="PropertySheets">
    <Import Project="$(SolutionDir)\global.targets" Condition="exists('$(SolutionDir)\global.targets')" />
  </ImportGroup>

  <ItemGroup>
    <Reference Include="Sandbox.Common">
      <SpecificVersion>False</SpecificVersion>
      <HintPath>$(BinPath)\Sandbox.Common.dll</HintPath>
    </Reference>
    <Reference Include="Sandbox.Game">
      <SpecificVersion>False</SpecificVersion>
      <HintPath>$(BinPath)\Sandbox.Game.dll</HintPath>
    </Reference>
    <Reference Include="SpaceEngineers.ObjectBuilders">
