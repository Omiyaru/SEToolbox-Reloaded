    </Reference>
    <Reference Include="SpaceEngineers.ObjectBuilders.XmlSerializers">
      <HintPath>$(BinPath)\SpaceEngineers.ObjectBuilders.XmlSerializers.dll</HintPath>
    </Reference>
    <Reference Include="VRage">
      <SpecificVersion>False</SpecificVersion>
      <HintPath>$(BinPath)\VRage.dll</HintPath>
    </Reference>
    <Reference Include="VRage.Game">
      <HintPath>$(BinPath)\VRage.Game.dll</HintPath>
    </Reference>
    <Reference Include="VRage.Game.XmlSerializers">
      <HintPath>$(BinPath)\VRage.Game.XmlSerializers.dll</HintPath>
    </Reference>
    <Reference Include="VRage.Library">
      <SpecificVersion>False</SpecificVersion>
      <HintPath>$(BinPath)\VRage.Library.dll</HintPath>
    </Reference>
    <Reference Include="VRage.Math">
      <SpecificVersion>False</SpecificVersion>
      <HintPath>$(BinPath)\VRage.Math.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
